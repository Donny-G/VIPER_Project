# Uncomment the next line to define a global platform for your project
platform :ios, '14.0'

target 'VIPER_Project' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for VIPER_Project
  pod 'RealmSwift', '~>10'

  target 'VIPER_ProjectTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RealmSwift', '~>10'
  end

  target 'VIPER_ProjectUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RealmSwift', '~>10'
  end

end
